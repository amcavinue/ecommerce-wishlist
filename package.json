{
  "name": "ecommerce-wishlist-app",
  "version": "0.0.1",
  "description": "An ecommerce wishlist app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "export NODE_ENV=development && run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "test:dev": "export NODE_ENV=test && mocha --recursive --compilers js:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "node debug server.js",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css css/index.css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "lessc --source-map less/index.less css/index.css && cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar less/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": "Aaron McAvinue",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.0.0",
    "enzyme": "^2.4.1",
    "http-server": "^0.9.0",
    "less": "^2.7.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.1",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "amazon-product-api": "^0.4.2",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "express-session": "^1.14.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.4",
    "jsonwebtoken": "^7.2.1",
    "mongoose": "^4.7.6",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.1",
    "react-validation": "^2.10.9",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "validator": "^6.2.1"
  }
}
